{
  "name": "rabbit",
  "version": "0.1.0",
  "description": "Testing with Canteen",
  "main": "?",
  "scripts": {
    "test": "node node_modules/karma/bin/karma start --single-run",
    "build": "npm run buildAmd | npm run buildBundle | npm run buildCommonJS | npm run buildSystemJS",
    "buildAmd": "babel src -d build/amd --presets=es2015 --plugins=transform-es2015-modules-amd",
    "buildBundle": "node node_modules/browserify/bin/cmd.js -r ./src/rabbit.js:rabbit --outfile build/bundle/rabbit.js -t [ babelify --presets [ es2015 ] ]",
    "buildCommonJS": "babel src -d build/commonjs --presets=es2015",
    "buildSystemJS": "babel src -d build/systemjs --presets=es2015 --plugins=transform-es2015-modules-systemjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cristiingineru/rabbit.git"
  },
  "keywords": [
    "canteen",
    "testing",
    "canvas"
  ],
  "author": "Cristi Ingineru",
  "license": "?",
  "bugs": {
    "url": "https://github.com/cristiingineru/rabbit/issues"
  },
  "homepage": "https://github.com/cristiingineru/rabbit",
  "devDependencies": {
    "Canteen": "1.0.4",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.18.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "jasmine-core": "^2.5.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "requirejs": "^2.3.2",
    "requirejs-babel": "0.0.9"
  }
}
